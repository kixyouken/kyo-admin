            "key": "province_id",
            "fields": ["province_id", "province_name"]
        },
        {
            "join": "inner",
            "table": "citys",
            "foreign": "city_id",
            "key": "city_id",
            "fields": ["city_id", "city_name"]
        },
        {
            "join": "inner",
            "table": "countys",
            "foreign": "county_id",
            "key": "county_id",
            "fields": ["county_id", "county_name"]
        }
    ]
}